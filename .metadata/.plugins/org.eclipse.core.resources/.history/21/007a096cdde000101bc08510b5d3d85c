/*
 * Sending Characters Using UART
 */
#include"stm32f4xx.h"

static void uart_config(void);

int main(void)
{
	uart_config();
}

static void uart_config(void)
{
	//clock for UART2
	RCC->APB1ENR |= (1U << 13);
	(void)RCC->APB1ENR;

	USART2->CR1 &= ~(1U << 13);  //disabled uart to configure

	USART3->BRR = 833.3125;
}
